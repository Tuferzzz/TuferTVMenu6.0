                </LinearLayout>
                <LinearLayout style="@style/third_layout_segmentation_style" />
            </LinearLayout>
            <LinearLayout android:orientation="vertical" android:focusable="false" android:layout_width="fill_parent" android:layout_height="fill_parent">
