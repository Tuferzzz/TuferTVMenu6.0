                <LinearLayout android:id="@+id/linearlayout_channel_colorsystem_pal" style="@style/third_layout1_item_style">
                    <TextView android:text="@string/str_mainmenu_channel_colorsystem_pal" style="@style/third_layout1_text_style" />
