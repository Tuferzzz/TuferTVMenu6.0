<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ethernet_ipv4_address_rl"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/ip_address_rl"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:minHeight="@dimen/x40dp" >

        <TextView
            android:layout_width="@dimen/x170dp"
            android:layout_height="@dimen/x28dp"
            android:paddingLeft="@dimen/x30dp"
            android:text="@string/ip_address"
            android:textColor="#CCCCCC"
            android:textSize="@dimen/sp22" />
		
        <tufer.com.menutest.UIActivity.network.networkfove.InputIPAddress
            android:id="@+id/ethernet_v4_ip_four"
            android:layout_width="@dimen/x50dp"
            android:layout_height="@dimen/x28dp"
            android:layout_alignParentRight="true"
            android:layout_marginRight="@dimen/x40dp"
            android:background="@drawable/edit_text"
            android:gravity="center"
            android:imeOptions="actionDone|flagNoExtractUi|flagNoFullscreen"
            android:maxLength="3"
            android:numeric="integer"
            android:textCursorDrawable="@drawable/cursor" />

        <tufer.com.menutest.UIActivity.network.networkfove.InputIPAddress
            android:id="@+id/ethernet_v4_ip_three"
            android:layout_width="@dimen/x50dp"
            android:layout_height="@dimen/x28dp"
            android:layout_alignParentTop="true"
            android:layout_marginRight="@dimen/x20dp"
            android:layout_toLeftOf="@+id/ethernet_v4_ip_four"
            android:background="@drawable/edit_text"
            android:ems="10"
            android:gravity="center"
            android:imeOptions="actionDone|flagNoExtractUi|flagNoFullscreen"
            android:maxLength="3"
            android:numeric="integer"
            android:textCursorDrawable="@drawable/cursor" />

        <tufer.com.menutest.UIActivity.network.networkfove.InputIPAddress
            android:id="@+id/ethernet_v4_ip_two"
            android:layout_width="@dimen/x50dp"
            android:layout_height="@dimen/x28dp"
            android:layout_alignParentTop="true"
            android:layout_marginRight="@dimen/x20dp"
            android:layout_toLeftOf="@+id/ethernet_v4_ip_three"
            android:background="@drawable/edit_text"
            android:ems="10"
            android:gravity="center"
            android:imeOptions="actionDone|flagNoExtractUi|flagNoFullscreen"
            android:maxLength="3"
            android:numeric="integer"
            android:textCursorDrawable="@drawable/cursor" />

        <tufer.com.menutest.UIActivity.network.networkfove.InputIPAddress
            android:id="@+id/ethernet_v4_ip_one"
            android:layout_width="@dimen/x50dp"
            android:layout_height="@dimen/x28dp"
            android:layout_alignParentTop="true"
            android:layout_marginRight="@dimen/x20dp"
            android:layout_toLeftOf="@+id/ethernet_v4_ip_two"
            android:background="@drawable/edit_text"
            android:ems="10"
            android:gravity="center"
            android:imeOptions="actionDone|flagNoExtractUi|flagNoFullscreen"
            android:maxLength="3"
            android:numeric="integer"
            android:textCursorDrawable="@drawable/cursor" >

            <requestFocus />
        </tufer.com.menutest.UIActivity.network.networkfove.InputIPAddress>
    </RelativeLayout>

